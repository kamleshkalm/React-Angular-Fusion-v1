{
  "name": "@react-angular-fusion/react",
  "version": "1.0.1",
  "description": "React adapter for React-Angular interoperability",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./angular-wrapper": {
      "require": "./dist/cjs/lib/AngularWrapper.js",
      "import": "./dist/esm/lib/AngularWrapper.js",
      "types": "./dist/types/lib/AngularWrapper.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kamleshkalm/React-Angular-Fusion-v1.git",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/kamleshkalm/React-Angular-Fusion-v1#README",
  "bugs": {
    "url": "https://github.com/kamleshkalm/React-Angular-Fusion-v1/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@angular/core": "^18.0.0 || ^19.0.0",
    "@angular/platform-browser": "^18.0.0 || ^19.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0 || ^19.0.0",
    "@angular/compiler": "^18.0.0 || ^19.0.0",
    "@react-angular-fusion/core": "^1.0.1"
  },
  "dependencies": {
    "@react-angular-fusion/core": "^1.0.1"
  },
  "devDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "@angular/core": "^18.0.0 || ^19.0.0",
    "@angular/platform-browser": "^18.0.0 || ^19.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0 || ^19.0.0",
    "@angular/compiler": "^18.0.0 || ^19.0.0",
    "@angular/compiler-cli": "^18.0.0 || ^19.0.0",
    "typescript": "^5.5.4",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": ["react", "angular", "wrapper", "interop","components"],
  "author": "Kamlesh Kumar",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ]
}